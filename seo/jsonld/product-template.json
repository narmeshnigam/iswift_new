{
  "@context": "https://schema.org",
  "@type": "Product",
  "name": "{Product Name}",
  "image": [
    "{Image URL 1}",
    "{Image URL 2}"
  ],
  "description": "{Short description of product}",
  "sku": "{SKU}",
  "brand": {
    "@type": "Brand",
    "name": "iSwift"
  },
  "offers": {
    "@type": "Offer",
    "priceCurrency": "INR",
